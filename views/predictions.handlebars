<!-- Search bar for searching individual ticker symbols -->
<section class="search">
    <form>
        <div class="input-group mb-3">
            <input type="text" name="tickerText" class="form-control" id="ticker-text" placeholder="Ticker Symbol" aria-label="Ticker Symbol"
                aria-describedby="basic-addon2">
            <div class="input-group-append">
                <button class="btn btn-outline-secondary" type="submit" id="ticker-search">Search</button>
            </div>
        </div>
    </form>
</section>

<!-- Modal for prediction -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"
    aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="ticker">Ticker Symbol</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <p id="chart">Chart goes here</p>
                <p id="ticker-description">Lorem ipsum, dolor sit amet consectetur adipisicing elit. Architecto
                    recusandae exercitationem et magnam voluptas vitae sit debitis sapiente laborum minima.</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" id="submitButton">Submit</button>
            </div>
        </div>
    </div>
</div>

